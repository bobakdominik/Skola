@page "/fetchdata"

<PageTitle>Weather forecast</PageTitle>

@using medicinska_informatika_SP.Data
@using medicinska_informatika_SP.Data.Models
@inject CalendarSercice CalendarSercice
<h1>Weather forecast</h1>

<p>This component demonstrates fetching data from a service.</p>

@if (forecasts == null)
{
    <p>
        <em>Loading...</em>
    </p>
}
else
{
    <table class="table">
        <thead>
        <tr>
            <th>Date</th>
            <th>Temp. (C)</th>
            <th>Temp. (F)</th>
            <th>Summary</th>
        </tr>
        </thead>
        <tbody>
        @foreach (var forecast in forecasts)
        {
            <tr>
                <td>@forecast.user</td>
                <td>@forecast.DateFrom</td>
                <td>@forecast.DateTo</td>
            </tr>
        }
        </tbody>
    </table>
}

@code {
    private IEnumerable<Holidays>? forecasts;

}